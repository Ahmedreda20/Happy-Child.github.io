.wrap-subscribe-banner
	.container
		.subscribe-banner(style="background-image: url(img/bg-subscribe-banner.jpg)")

			.row.justify-content-center.align-items-stretch.subscribe-banner-content

				.col-12.col-sm-9.col-md-10.col-lg-4
					.wrap-text
						span.subtitle.text-fw-medium Подпишись на новости и получи
						span.title.text-fw-bold скидку 5%

				.w-100.d-block.d-sm-none

				.col-12.col-sm-9.col-md-10.col-lg-6
					form.form-validate
						.wrap-field
							input(required, type="text", name="email", placeholder="E-mail", data-msg-email="Неверный формат")
							.error-icon
								img(src="img/icon-error.svg", alt="")

						button(type="submit").btn.btn-dark подписаться

						.wrap-checkbox
							input(required, type="checkbox", id="tdbanner", name="agree")
							label(for="tdbanner").box
							span.text.text-medium Согласен с обработкой 
								em персональных данных

