.wrap-fixed-bar

	.wrap-btns
		.wrap-btn
			.btn(data-show-key="show-basket")
				.wrap-icon.img-resp
					img(src="img/icon-shopping-cart.svg", alt="")
				span.count 10
			.btn-label В корзине товаров<br>на сумму:
				span.total-price  50 000.00
				|  руб.

		.wrap-btn
			.btn(data-show-key="show-deferred")
